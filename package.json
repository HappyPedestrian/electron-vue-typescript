{
  "name": "electron-vite-ts",
  "productName": "electron-vite-ts",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "scripts": {
    "start:dev": "cross-env VITE_CONFIG_MODE=development electron-forge start",
    "start:test": "cross-env VITE_CONFIG_MODE=test electron-forge start",
    "start:prod": "cross-env VITE_CONFIG_MODE=production electron-forge start",
    "package": "electron-forge package",
    "make:dev": "cross-env VITE_CONFIG_MODE=development electron-forge make",
