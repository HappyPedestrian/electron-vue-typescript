    "@vitejs/plugin-vue": "^5.2.1",
    "cross-env": "^7.0.3",
    "electron": "34.1.1",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "sass-embedded": "^1.82.0",
    "ts-node": "^10.0.0",
